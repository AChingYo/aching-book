{
	"plugins": [  "anchor-navigation-ex", "edit-link", "copy-code-button", "theme-comscore" ],
	"pluginsConfig": {
		"fontsettings": {
			"theme": "night",
			"family": "sans"
		},
		"anchor-navigation-ex": {
			"showLevel": true,
			"associatedWithSummary": false,
			"printLog": false,
			"multipleH1": false,
			"mode": "float",
			"showGoTop":true,
			"float": {
				"floatIcon": "fa fa-navicon",
				"showLevelIcon": false,
				"level1Icon": "fa fa-hand-o-right",
				"level2Icon": "fa fa-hand-o-right",
				"level3Icon": "fa fa-hand-o-right"
			},
			"pageTop": {
				"showLevelIcon": false,
				"level1Icon": "fa fa-hand-o-right",
				"level2Icon": "fa fa-hand-o-right",
				"level3Icon": "fa fa-hand-o-right"
			}
		},
		"edit-link": {
			"base": "https://github.com/AChingYo/aching-book/edit/master/book_sources/",
			"label": "Edit This Page"
		}
	}
}
